<?xml version="1.0" encoding="UTF-8"?>
<WMS_Capabilities version="1.3.0" xmlns="http://www.opengis.net/wms" xmlns:xlink="http://www.w3.org/1999/xlink" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.opengis.net/wms http://schemas.opengis.net/wms/1.3.0/capabilities_1_3_0.xsd">
  <Service>
    <Name>WMS</Name>
    <Title>Hillshade WMS Service</Title>
    <Abstract>A sample WMS service for demonstration purposes</Abstract>
    <KeywordList>
      <Keyword>hillshade</Keyword>
      <Keyword>elevation</Keyword>
      <Keyword>wms</Keyword>
    </KeywordList>
    <OnlineResource xlink:type="simple" xlink:href="http://example.com/wms"/>
    <Fees>none</Fees>
    <AccessConstraints>none</AccessConstraints>
    <ContactInformation>
      <ContactPersonPrimary>
        <ContactPerson>Anand Mahesh</ContactPerson>
        <ContactOrganization>Capsule Corp</ContactOrganization>
      </ContactPersonPrimary>
      <ContactPosition>Software Engineer</ContactPosition>
      <ContactVoiceTelephone>+91-9876543210</ContactVoiceTelephone>
      <ContactElectronicMailAddress>manand881@gmail.com</ContactElectronicMailAddress>
    </ContactInformation>
  </Service>
  <Capability>
    <Request>
      <GetCapabilities>
        <Format>application/xml</Format>
        <DCPType>
          <HTTP>
            <Get><OnlineResource xlink:type="simple" xlink:href="http://example.com/wms?"/></Get>
          </HTTP>
        </DCPType>
      </GetCapabilities>
      <GetMap>
        <Format>image/png</Format>
        <DCPType>
          <HTTP>
            <Get><OnlineResource xlink:type="simple" xlink:href="http://example.com/wms?"/></Get>
          </HTTP>
        </DCPType>
      </GetMap>
      <GetFeatureInfo>
        <Format>text/plain</Format>
        <Format>application/vnd.ogc.gml</Format>
        <DCPType>
          <HTTP>
            <Get><OnlineResource xlink:type="simple" xlink:href="http://example.com/wms?"/></Get>
          </HTTP>
        </DCPType>
      </GetFeatureInfo>
    </Request>
    <Exception>
      <Format>XML</Format>
      <Format>INIMAGE</Format>
      <Format>BLANK</Format>
    </Exception>
    <Layer>
      <Title>Layers</Title>
      <CRS>EPSG:4326</CRS>
      <Layer queryable="1" opaque="0" noSubsets="0" cascaded="0">
        <Name>dsm</Name>
        <Title>Digital Surface Model</Title>
        <Abstract>Single-band Digital Surface Model representing hillshade surface</Abstract>
        <CRS>EPSG:4326</CRS>
        <BoundingBox CRS="EPSG:4326" minx="10.0" miny="20.0" maxx="30.0" maxy="40.0"/>
      </Layer>
    </Layer>
  </Capability>
</WMS_Capabilities>
